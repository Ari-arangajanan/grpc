create database seata_book;

use seata_book;

CREATE TABLE `book_tbl` (
  `id` bigint(20) NOT NULL AUTO_INCREMENT COMMENT 'increment id',
  `name` varchar(100) NOT NULL COMMENT 'name',
  `author_id` bigint(20) NOT NULL COMMENT 'account id',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=1 DEFAULT CHARSET=utf8;


alter table book_tbl rename column authorId to author_id;


createBook
curl --location 'http://localhost:9302/book' \
--header 'Content-Type: application/json' \
--data '{
    "name":"ICT",
    "authorId":1
}'